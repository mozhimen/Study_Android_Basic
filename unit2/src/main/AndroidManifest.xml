<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Study_Android_Basic"
        tools:targetApi="31">
        <activity
            android:name="com.mozhimen.study_basic_unit2.recyclerview.RecyclerView2Activity"
            android:exported="false" />
        <activity
            android:name="com.mozhimen.study_basic_unit2.recyclerview.RecyclerViewActivity"
            android:exported="false" />
        <activity
            android:name="com.mozhimen.study_basic_unit2.TipCalculateActivity"
            android:exported="false" />
        <activity
            android:name="com.mozhimen.study_basic_unit2.LemonadeActivity"
            android:exported="false" />
        <activity
            android:name="com.mozhimen.study_basic_unit2.com.mozhimen.study_basic_unit1.DiceRollerActivity"
            android:exported="false" />
        <activity
            android:name="com.mozhimen.study_basic_unit2.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.mozhimen.study_basic_unit2.dogglers.VerticalListActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mozhimen.study_basic_unit2.MainActivity" />
        </activity>
        <activity android:name="com.mozhimen.study_basic_unit2.dogglers.HorizontalListActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mozhimen.study_basic_unit2.MainActivity" />
        </activity>
        <activity android:name="com.mozhimen.study_basic_unit2.dogglers.GridListActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mozhimen.study_basic_unit2.MainActivity" />
        </activity>
        <activity
            android:name="com.mozhimen.study_basic_unit2.dogglers.DogglersActivity"
            android:exported="false" />
    </application>

</manifest>